{
	"name": "@jdeighan/unit-tester",
	"version": "1.0.4",
	"type": "module",
	"description": "A unit testing framework",
	"exports": {
		".": "./src/UnitTester.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "cls && coffee -c .",
		"test": "npm run build && ava ./test/*.test.js",
		"publish": "npm publish --access=public"
	},
	"ava": {
		"verbose": false
	},
	"keywords": [
		"coffeescript",
		"unit test"
	],
	"author": "John Deighan",
	"license": "MIT",
	"dependencies": {
		"@jdeighan/coffee-utils": "^5.0.1",
		"ava": "^3.15.0",
		"coffeescript": "^2.6.1"
	}
}
