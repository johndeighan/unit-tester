{
	"name": "@jdeighan/unit-tester",
	"version": "2.0.30",
	"type": "module",
	"description": "A unit testing framework",
	"exports": {
		".": "./src/UnitTester.js",
		"./utils": "./src/utils.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "cls && coffee -c .",
		"test": "npm run build && ava ./test/*.test.js",
		"publish": "npm publish --access=public"
	},
	"ava": {
		"verbose": false
	},
	"keywords": [
		"coffeescript",
		"unit test"
	],
	"author": "John Deighan",
	"license": "MIT",
	"dependencies": {
		"@jdeighan/exceptions": "^1.0.7",
		"ava": "^4.3.3",
		"coffeescript": "^2.7.0"
	}
}
